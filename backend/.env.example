# Environment Configuration
NODE_ENV=production

# MongoDB Configuration - IMPORTANT: Include /meditech database name
MONGODB_URI=mongodb+srv://USERNAME:PASSWORD@CLUSTER.mongodb.net/meditech?retryWrites=true&w=majority&appName=APPNAME

# JWT Configuration - Use a strong 64+ character secret
JWT_SECRET=YOUR_SECURE_JWT_SECRET_64_CHARACTERS_MINIMUM

# Frontend URL (for CORS) - Update with your actual frontend URL
FRONTEND_URL=https://your-frontend-domain.vercel.app

# Blockchain Configuration (Sepolia)
INFURA_API_KEY=YOUR_INFURA_API_KEY
INFURA_API_SECRET=YOUR_INFURA_API_SECRET
INFURA_NETWORK=sepolia
CONTRACT_ADDRESS=YOUR_CONTRACT_ADDRESS
ADMIN_PRIVATE_KEY=YOUR_ADMIN_PRIVATE_KEY
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID

# IPFS Configuration (Pinata)
PINATA_API_KEY=YOUR_PINATA_API_KEY
PINATA_SECRET_KEY=YOUR_PINATA_SECRET_KEY
